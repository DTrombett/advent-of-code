{
	"description": "My solutions to the Advent of Code",
	"exports": "./dist/index.js",
	"main": "./dist/index.js",
	"name": "advent-of-code",
	"author": "DTrombett",
	"license": "MIT",
	"type": "module",
	"version": "0",
	"scripts": {
		"build": "tsup",
		"dev": "cross-env NODE_ENV=development npm run build && node --enable-source-maps .",
		"lint": "eslint src --fix",
		"prettier": "prettier --write src/**/*.ts",
		"start": "npm run build && node .",
		"test": "tsc --noEmit && eslint src"
	},
	"engines": {
		"node": ">=22"
	},
	"devDependencies": {
		"@eslint/js": "^9.16.0",
		"@tsconfig/node22": "^22.0.0",
		"@tsconfig/recommended": "^1.0.8",
		"@tsconfig/strictest": "^2.0.5",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^22.10.1",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.5",
		"eslint": "^9.16.0",
		"eslint-plugin-n": "^17.14.0",
		"prettier": "^3.4.1",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	}
}
